    "aos": "3.0.0-beta.6",
    "iconify-icon": "^1.0.8",
    "longdo-map-vue": "^3.0.2",
    "pinia": "^2.1.7",
    "sweetalert2": "^11.10.2",
    "vue": "^3.4.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tailwindcss/typography": "^0.5.10",
